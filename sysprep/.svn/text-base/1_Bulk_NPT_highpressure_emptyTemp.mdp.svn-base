; VARIOUS PREPROCESSING OPTIONS = 
title                    = Protein-water
cpp                      = /lib/cpp

; RUN CONTROL PARAMETERS = 
integrator               = md
tinit                    = 000
dt                       = 0.001
nsteps                   = 50 ;0.5ps
comm-mode                = Linear
; number of steps for center of mass motion removal
 nstcomm                  = 1
; group(s) for center of mass motion removal
energy_grps = SED_energygrps_SED

; OUTPUT CONTROL OPTIONS = 
nstxout                  = 0
nstvout                  = 0
nstfout                  = 0
nstlog                   = 1000
nstenergy                = 1000
nstxtcout                = 1000
xtc_precision            = 1000
xtc_grps                 = 


; NEIGHBORSEARCHING PARAMETERS = 
nstlist                  = 10
ns_type                  = grid
pbc                      = xyz
;periodic_molecules       = yes
rlist                    = 2.9 ;3.0

; OPTIONS FOR ELECTROSTATICS AND VDW = 
coulombtype              = PME
rcoulomb                 = 2.9 ; 3.0
rcoulomb_switch          = 1.0
vdw_type                 = Shift 
rvdw                     = 2.6 
fourierspacing           = 0.12
pme_order                = 4
ewald_rtol               = 1e-05
optimize_fft             = yes


; OPTIONS FOR WEAK COUPLING ALGORITHMS = 
tcoupl                   = v-rescale
tc-grps	                 = System 
tau_t                    = 1.0
ref_t                    = SED_temperature_name_SED
Pcoupl                   = Berendsen 
Pcoupltype               = isotropic
tau_p                    = 1.0
compressibility          = 4.5e-5  ; 1e-5 0 0 0
ref_p                    = 10000.0     ; bar

; GENERATE VELOCITIES FOR STARTUP RUN = 
gen_vel                  = no ;yes
gen_temp                 = SED_temperature_name_SED
gen_seed                 = 473529

; OPTIONS FOR BONDS     = 
constraints              = hbonds 
constraint_algorithm     = lincs
unconstrained_start      = no
shake_tol                = 0.00001
lincs_order              = 4
lincs_warnangle          = 30
morse                    = no
lincs_iter               = 2


